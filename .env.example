# Firebase web app config
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Google OAuth (Expo AuthSession)
# Create OAuth client IDs in Google Cloud Console:
# - Expo client (Web type) for Expo Go proxy
# - iOS client for standalone iOS builds
# - Android client for standalone Android builds
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# Auth domains
# Ensure Firebase Authentication -> Settings -> Authorized domains includes:
# localhost, 127.0.0.1, and your tunnel domain (e.g., <id>.ngrok.app) if used.